{
    "name": "watson/bootstrap-form",
    "description": "Laravel 5 form wrappers for Bootstrap 3.",
    "keywords": [
        "laravel",
        "form",
        "bootstrap"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Dwight Watson",
            "email": "dwightwatson@me.com"
        }
    ],
    "require": {
        "php": "^8.0",
        "illuminate/config": "~7.0 | ~8.0",
        "illuminate/session": "~7.0 | ~8.0",
        "illuminate/support": "~7.0 | ~8.0",
        "laravelcollective/html": "~6.0"
    },
    "require-dev": {
        "mockery/mockery": "~1.3",
        "phpunit/phpunit": "~9.0"
    },
    "autoload": {
        "psr-4": {
            "Watson\\BootstrapForm\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Watson\\BootstrapForm\\BootstrapFormServiceProvider"
            ],
            "aliases": {
                "BootForm": "Watson\\BootstrapForm\\Facades\\BootstrapForm"
            }
        }
    }
}
